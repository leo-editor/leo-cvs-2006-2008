<t tx="ekr.20070626112754.649">def findSectionName(self,s,i):

    end = s.find('\n',i)
    if end == -1:
        n1 = s.find("&lt;&lt;",i)
        n2 = s.find("&gt;&gt;",i)
    else:
        n1 = s.find("&lt;&lt;",i,end)
        n2 = s.find("&gt;&gt;",i,end)

    ok = -1 &lt; n1 &lt; n2

    # New in Leo 4.4.3: warn on extra brackets.
    if ok:
        for ch,j in (('&lt;',n1+2),('&gt;',n2+2)):
            if g.match(s,j,ch):
                line = g.get_line(s,i)
                g.es('dubious brackets in %s' % line)
                break

    return ok, n1, n2
