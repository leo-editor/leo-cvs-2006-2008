<t tx="ekr.20070626112754.1519">def createElispDataNode (self,p,s):

    data = s
    # g.trace(len(data))

    # Skip blank lines and comment lines.
    i = 0
    while i &lt; len(s):
        i = g.skip_ws_and_nl(s,i)
        if g.match(s,i,';'):
            i = g.skip_line(s,i)
