<t tx="ekr.20070626112754.1644">def isMethod (self,s):

    '''Return True if the test_ method's first arg is self.'''

    if s.find('self') == -1:
        return False

    name = self.name
    i = s.find('def %s' % name)
    if i == -1:
        g.trace('%s not defined' % name)
    else:
        i += (4 + len(name))
        i = g.skip_ws(s,i)
        if g.match(s,i,'('):
