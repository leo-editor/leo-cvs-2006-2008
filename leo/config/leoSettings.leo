<t tx="ekr.20060112104823">def main ():
    
    global setNames
    
    root = u.findNodeAnywhere('Keyboard shortcuts')

    for p in root.self_and_subtree_iter():
        for name in setNames:
            if p.headString().startswith(name):
                doSet(p,name)
                
    checkSets()
