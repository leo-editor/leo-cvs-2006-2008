<t tx="ekr.20060513122450.102">def AddPath(self,name):
    d = tkFileDialog.askdirectory()
    if d != "":
        d = d.replace("/","\\")
        for o in self.Objects:
            if o.Name == name:
                opaths = o.Get().splitlines()
                npaths = []
                
                for p in opaths:
                    p = p.strip()
                    if p != "":
                        npaths.append(p)
                        
                npaths.append(d)
                
                o.Set(string.join(npaths,"\n"))</t>
